#!groovy

def provisioner() {
  stage('Infra sync') {
    checkout scm
  }

  stage('Build Environment Space') {
  sh """#!/bin/bash -x
      cf orgs | grep org_name
    """
  }

  stage('Secure Environment Space') {
      sh """#!/bin/bash -x
          cf orgs | grep org_name
        """
  }

}

node('Chef Provisioner Prod') {
  provisioner()
}
